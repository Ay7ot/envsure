{
  "name": "envsure",
  "version": "1.0.0",
  "description": "A lightweight CLI tool for validating and comparing .env files",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "envsure": "./dist/bin/envsure.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/envsure.js",
    "dev": "tsc --watch",
    "test": "npm run build && node --test dist/tests/*.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "env",
    "dotenv",
    "environment",
    "variables",
    "validation",
    "cli",
    "config"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ay7ot/envsure"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
